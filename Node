public class Node {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
	public class MyNode{
		public String str;
		public Node next;
		String[] s = null; 


		public  MyNode(String a,Node n){
			str=a;
			next=n;
		}
		
		public Node getNod() {
			return next;
		}

		public void setNod(Node nod) {
			this.next = nod;
		}
		
		public String getStr() {
			return str;
		}

		public void setStr(String str) {
			this.str = str;
		}
		
		public void addNextNode(String a,Node n){
			MyNode NewNode=new MyNode(String b,Node m);
			b=a;
			m=n;
		}
		
		
		
		// setString removeNextNode  getNextNode printLink
		
	}

}
