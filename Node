
public class Node {

	public static void main(String[] args) {
		MyNode nod = new MyNode("dog");
		

	}

}	
	private static MyNode nod;
	
	public class MyNode{
		public String str;
		public MyNode no;
		String[] s = null; 
		public Node next;


		public  MyNode(String a){
			str=a;
		}
		
		
		public String getStr() {
			return str;
		}

		public void setStr(String str) {
			this.str = str;
		}
	
		public void addNextNode(MyNode n){
			no=n;
		}
		
		public MyNode getNextNode(Node n){
			if(no==null) return null;
			else return no;
		}
		
		public void removeNextNode(Node n) {
			if(no==null) System.out.println("error");
			else no=null;
		}
		
		public void printLink(){
			System.out.println(str);
		}
		
	}


